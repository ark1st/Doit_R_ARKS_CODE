install.package(ggplot2)
mpg <- as.data.frame(ggplot2::mpg)

# 1. head() -  데이터 앞부분 출력
head(mpg)
head(mpg,10) #10행까지 출력

# 2. tail() -  데이터 뒷부분 출력
tail(mpg)
tail(mpg,10) #뒤에서 10행

# 3. View() - 뷰어 창에서 확인
View(mpg)

# 4. dim() - 데이터의 차원 출력
dim(mpg)

# 5. str() - 속성 확인
str(mpg)

# 6. summary() - 요약 통계량 산출
summary(mpg)
